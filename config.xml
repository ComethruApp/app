        <edit-config file="*-Info.plist" mode="merge" target="NSLocationWhenInUseUsageDescription">
            <string>Finding current event.</string>
        </edit-config>
        <edit-config file="*-Info.plist" mode="merge" target="NSLocationAlwaysUsageDescription">
            <string>We use your location, with your consent, to detect which events you are attending.</string>
        </edit-config>
        <config-file parent="NSCameraUsageDescription" platform="ios" target="*-Info.plist">
            <string>For adding header photos.</string>
        </config-file>
        <framework spec="5.7.0" src="FBSDKCoreKit" type="podspec" />
        <framework spec="5.7.0" src="FBSDKLoginKit" type="podspec" />
        <framework spec="5.7.0" src="FBSDKShareKit" type="podspec" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="1.3.3" />
    <plugin name="cordova-plugin-statusbar" spec="2.4.2" />
    <plugin name="cordova-plugin-device" spec="2.0.2" />
    <plugin name="cordova-plugin-splashscreen" spec="5.0.2" />
    <plugin name="cordova-plugin-ionic-keyboard" spec="^2.0.5" />
    <plugin name="cordova-plugin-app-preferences" spec="https://github.com/vash15/me.apla.cordova.app-preferences" />
    <plugin name="cordova-plugin-inappbrowser" spec="https://github.com/kleeb/cordova-plugin-inappbrowser.git" />
    <plugin name="cordova-plugin-camera" spec="^4.1.0">
        <variable name="ANDROID_SUPPORT_V4_VERSION" value="27.+" />
        <variable name="CAMERA_USAGE_DESCRIPTION" value="To choose event headers" />
        <variable name="PHOTOLIBRARY_USAGE_DESCRIPTION" value="To choose event headers" />
    </plugin>
    <plugin name="cordova-plugin-file" spec="^6.0.2" />
    <plugin name="cordova-plugin-ionic-webview" spec="^4.1.3">
        <variable name="ANDROID_SUPPORT_ANNOTATIONS_VERSION" value="27.+" />
    </plugin>
    <plugin name="cordova-sqlite-storage" spec="^3.4.1" />
    <plugin name="cordova-plugin-filepath" spec="^1.5.7" />
    <engine name="android" spec="8.1.0" />
    <engine name="ios" spec="5.1.1" />
</widget>
